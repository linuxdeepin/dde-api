#!/usr/bin/make -f
export GOCACHE = /tmp/gocache
export GOPATH := /usr/share/gocode

%:
	dh $@ --with systemd

ifeq ($(DEB_BUILD_ARCH),sw_64)
override_dh_strip:
	true
override_dh_auto_test:
	true
else
override_dh_strip:
	dh_strip --dbgsym-migration=dde-api-dbg
endif

override_dh_systemd_start:
	true
